-- Action değerlerinin dağılımı
SELECT action, COUNT(*) as count FROM incidents GROUP BY action ORDER BY count DESC;

-- Destination değerlerinin durumu
SELECT destination, COUNT(*) as count FROM incidents GROUP BY destination ORDER BY count DESC LIMIT 10;

-- Örnek bir kayıt
SELECT id, action, destination, file_name, login_name, email_address, violation_triggers 
FROM incidents LIMIT 3;
