
# Token'ı değişkene ata (CRLF olmadan)
$token = "TOKENINI_TEK_SATIRDA_YAZ"

# curl ile dene
curl.exe -k -X POST "https://172.16.245.126:9443/dlp/rest/v1/incidents/" -H "Authorization: Bearer $token" -H "Content-Type: application/json" -d "{\"type\":\"INCIDENTS\",\"from_date\":\"11/12/2025 10:00:00\",\"to_date\":\"11/12/2025 12:00:00\",\"start\":0,\"limit\":10}" --max-time 120
